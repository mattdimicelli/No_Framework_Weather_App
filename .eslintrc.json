{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": "warn"
    },
    "ignorePatterns": [
        "node_modules",
        "build",
        "dist",
        "public"
      ]
}